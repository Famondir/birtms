 abstract = {Context: When working with health-related questionnaires, outlier detection is important. However, traditional methods of outlier detection (e.g., boxplots) can miss participants with {\textquotedbl}atypical{\textquotedbl} responses to the questions that otherwise have similar total (subscale) scores. In addition to detecting outliers, it can be of clinical importance to determine the reason for the outlier status or {\textquotedbl}atypical{\textquotedbl} response. Objective: The aim of the current study was to illustrate how to derive person fit statistics for outlier detection through a statistical method examining person fit with a health-based questionnaire. Design and Participants: Patients treated for Cushing's syndrome (n = 394) were recruited from the Cushing's Support and Research Foundation's (CSRF) listserv and Facebook page. Main Outcome Measure: Patients were directed to an online survey containing the CushingQoL (English version). A two-dimensional graded response model was estimated, and person fit statistics were generated using the Zh statistic. Results: Conventional outlier detections methods revealed no outliers reflecting extreme scores on the subscales of the CushingQoL. However, person fit statistics identified 18 patients with {\textquotedbl}atypical{\textquotedbl} response patterns, which would have been otherwise missed (Zh {\textgreater} |$\pm$2.00|). Conclusion: While the conventional methods of outlier detection indicated no outliers, person fit statistics identified several patients with {\textquotedbl}atypical{\textquotedbl} response patterns who otherwise appeared average. Person fit statistics allow researchers to delve further into the underlying problems experienced by these {\textquotedbl}atypical{\textquotedbl} patients treated for Cushing's syndrome. Annotated code is provided to aid other researchers in using this method.},
 author = {Felt, John M. and Castaneda, Ruben and Tiemensma, Jitske and Depaoli, Sarah},
 year = {2017},
 title = {Using Person Fit Statistics to Detect Outliers in Survey Research},
 pages = {863},
 volume = {8},
 issn = {1664-1078},
 journal = {Frontiers in psychology},
 doi = {10.3389/fpsyg.2017.00863},
 file = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5445123},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/28603512}
}


