 booktitle = {Methodological issues of longitudinal surveys},
 year = {2016},
 address = {Wiesbaden},
 doi = {10.1007/978-3-658-11994-2{\textunderscore }29},
 file = {0781c6a2-89e1-40a8-bf2a-24560219a62c:C\:\\Users\\Simon\\AppData\\Local\\Swiss Academic Software\\Citavi 6\\ProjectCache\\gpx0bdjkrqedh3cub1p6ii0z8cdi3kwx13dhyhzvh7vcg\\Citavi Attachments\\0781c6a2-89e1-40a8-bf2a-24560219a62c.pdf:pdf}
